# Cargamos i
LOAD =0
STORE 2
# Cargamos n
LOAD =10
STORE 4

for_i:          # for (int i = 0; i < n - 1; i++)                
                LOAD 4
                SUB =1
                SUB 2
                JGTZ carga_j
                JUMP fin

carga_j:        # for (int j = n - 1; j > 1; j--)
                LOAD 4
                SUB =1
                STORE 3

for_j:          WRITE 3
                LOAD 3
                SUB 2
                SUB =1
                JZERO fin_for_j

                # j = j - 1
                LOAD 3
                SUB =1
                STORE 3
                JUMP for_j

fin_for_j:      # i = i + 1
                LOAD 2
                ADD =1
                STORE 2

                JUMP for_i

fin:    HALT
