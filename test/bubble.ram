# Cargamos i
LOAD =0
STORE 2
# Cargamos n
LOAD =20
STORE 4

for_i:          # for (int i = 0; i < n - 1; i++)                
                LOAD 4
                SUB =1
                SUB 2
                JGTZ carga_j
                JUMP fin

carga_j:        # for (int j = n - 1; j > 1; j--)
                LOAD 4
                SUB =1
                STORE 3

                # ComprobaciÃ³n j
for_j:          LOAD 3
                SUB 2
                JZERO fin_for_j

                # if (A[j - 1] > A[j])
                LOAD 3
                SUB =1
                STORE 6
                LOAD R1[*3]
                STORE 7
                LOAD R1[*6]
                SUB 7
                JGTZ check
                JUMP end_check

check:          # int temp = A[j - 1]
                LOAD R1[*6]
                STORE 5

                # A[j - 1] = A[j]
                LOAD R1[*3]
                STORE R1[*6]

                # A[j] = temp
                LOAD 5
                STORE R1[*3]

end_check:
                # j = j - 1
                LOAD 3
                SUB =1
                STORE 3
                JUMP for_j

fin_for_j:      
                # i = i + 1
                LOAD 2
                ADD =1
                STORE 2

                JUMP for_i

fin:    HALT
