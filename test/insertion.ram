# Cargamos lo necesario
LOAD =1
STORE 2
LOAD =10
STORE 4

for:                                      
    LOAD 4
    SUB 2
    JZERO end_for

    LOAD R1[*2]                     # Cargamos A[j]
    STORE 5                         # key = A[j]
    LOAD 2                          # i = j - 1
    SUB =1
    STORE 3

    # Bucle while
    # Comprobar que i > 0
    while:                          
        LOAD 3
        JZERO end_while            # Salta al final del bucle si i <= 0

        # A[i] > key
        LOAD R1[*3]
        SUB *5
        JGTZ end_while             # Salta al final del bucle si A[i] > key

        # Actualizamos i y volvemos al bucle while
        LOAD 3          
        SUB =1
        STORE 3
        JUMP while                  # Vuelve al inicio del bucle while

    end_while:                      
        LOAD 3
        ADD =1
        STORE 6
        LOAD 5
        STORE R1[*6]

        LOAD 2
        ADD =1
        STORE 2
        JUMP for                    # Vuelve al inicio del bucle for

end_for:                            
    HALT
