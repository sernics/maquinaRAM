# Cargamos j
LOAD =1
STORE 2
# Cargamos tamaño de n
LOAD =10
ADD =1
STORE 4

            # Cargamos n - 1
for:        LOAD 4
            SUB =1
            # ((n - 1) - j) == 0 -> jump end_for
            SUB 2
            JZERO end_for

            # key = A[j]
            LOAD R1[*2]
            STORE 5
            
            # i = j - 1
            LOAD 2
            SUB =1
            STORE 3

            # i >= 0
while:      LOAD 3
            JZERO second
            JGTZ second
            JUMP end_while
          
second:     # A[i] > key
            LOAD R1[*3]
            SUB 5
            JGTZ continue
            JUMP end_while

continue:   # A[i + 1] = A[i]
            LOAD 3
            ADD =1
            STORE 6
            LOAD R1[*3]
            STORE R1[*6]

            # i = i - 1
            LOAD 3
            SUB =1
            STORE 3
            JUMP while

            # Para que retorne después de evaluar el 0 si fuese necesario
            LOAD 3
            JZERO end_while
            
            # A[i + 1] = key
end_while:  LOAD 3
            ADD =1
            STORE 6
            LOAD 5
            STORE R1[*6]

            # j = j + 1
            LOAD 2
            ADD =1
            STORE 2
            JUMP for


end_for:    HALT
