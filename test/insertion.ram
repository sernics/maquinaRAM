# Cargamos j
LOAD =1
STORE 2
# Cargamos tamaño de n
LOAD =11
STORE 4

            # Cargamos n - 1
for:        LOAD 4
            SUB =1
            # ((n - 1) - j) == 0 -> jump end_for
            SUB 2
            JZERO end_for
            WRITE 2

            # key = A[j]
            LOAD R1[*2]
            STORE 5
            
            # i = j - 1
            LOAD 2
            SUB =1
            STORE 3

            # i >= 0
while:      LOAD 3
            WRITE 3
            SUB =0
            JZERO continue
            JGTZ continue
            JUMP end_while

continue:   LOAD 3
            JZERO end_while

            # i = i - 1
            LOAD 3
            SUB =1
            STORE 3
            JUMP while
            
            # A[i + 1] = key
end_while:  LOAD 3
            ADD =1
            STORE 6
            LOAD 5
            STORE R1[*6]

            # j = j + 1
            LOAD 2
            ADD =1
            STORE 2
            JUMP for


end_for:    HALT
